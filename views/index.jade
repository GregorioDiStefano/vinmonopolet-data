html(ng-app="VinData")
  head
    title!= title
    script(src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.5/angular.min.js")
    script(src="/static/ui-bootstrap-tpls.min.js")
    script(src="/static/lookup.js")
    script(src="/static/Chart.js")
    script(src="/static/angular-chart.min.js")

    link(rel='stylesheet', href='/static/pretty_table.css')
    link(rel='stylesheet', href='/static/ui-bootstrap-csp.css')
    link(rel='stylesheet', href='/static/bootstrap-combined.min.css')
    link(rel='stylesheet', href='/static/angular-chart.min.css')

  body
    h1!= "Vinmonopolet price history"
    div(class="container-fluid", ng-controller="TypeaheadCtrl")
        input(type="text", ng-model="result", typeahead-on-select="set_item($item)", typeahead="suggestion for suggestion in product($viewValue)")
    table(cellspacing=0)
        thead
            tr
                th!="Varenummer"
                th!="Varenavn"
        tbody
            each val, index in diff_products
                tr
                    td
                        a(href=val.vareurl)!=val.varenummer
                    td!=val.varenavn
